        :hasOperation          :PrediktorAutoOrchOper ;
        :hasServiceDefinition  "AutonomicOrchestrationPush" .

:PrediktorAutoOrchOper
        :hasAddress  "http://localhost:6565/pull/orchestration/auto" .

